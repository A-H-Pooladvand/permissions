{
    "name": "amirhossein/permissions",
    "description": "This package provides a flexible way to add Role-based Permissions to Laravel",
    "keywords": ["acl", "authorization", "laravel","laratrust","permissions","php", "rbac","roles", "multiusers", "teams"],
    "license": "MIT",
    "authors": [
        {
            "name": "Amirhossein Pooladvand",
            "homepage": "https://github.com/A-H-Pooladvand/permissions"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/auth": "~5.2",
        "illuminate/cache": "~5.2",
        "illuminate/console": "~5.2",
        "illuminate/database": "^5.2.32",
        "illuminate/support": "~5.2",
        "kkszymanowski/traitor": "^0.2.0"
    },
    "require-dev": {
        "mockery/mockery": ">=0.9.9",
        "phpunit/phpunit": ">=4.1",
        "orchestra/testbench": "~3.2"
    },
    "autoload": {
        "psr-4": {
            "Permission\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Permission\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Permission\\LaratrustServiceProvider"
            ],
            "aliases": {
                "Permission": "Laratrust\\LaratrustFacade"
            }
        }
    }
}
